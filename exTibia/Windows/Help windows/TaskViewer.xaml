<Window x:Class="exTibia.TaskViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Icon="/exTibia;component/Graphics/icon.ico"
        Title="TaskViewer" Height="284" Width="328">
    <Grid>
        <ListBox Height="215" HorizontalAlignment="Left" Name="listBox1" VerticalAlignment="Top" Width="306" Margin="0,30,0,0">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <Grid Height="25">
                        <TextBlock Text="{Binding Path=Value}" />
                        <TextBlock Text="{Binding Path=Key.Name}" />
                        
                    </Grid>
                </DataTemplate>
            </ListBox.ItemTemplate>


        </ListBox>



    </Grid>
</Window>
