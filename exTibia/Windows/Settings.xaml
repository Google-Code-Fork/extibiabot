<Window x:Class="exTibia.Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
        Title="Settings"
        Height="350"
        Width="550"
        WindowStyle="ToolWindow"
        ResizeMode="NoResize"
        xmlns:local="clr-namespace:exTibia"
        xmlns:System="clr-namespace:System;assembly=mscorlib"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        Closing="Window_Closing"
        WindowStartupLocation="CenterOwner"
        Icon="/exTibia;component/Graphics/icon.ico">
    <Window.Resources>
        
        <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="ConstsDirections">
            <ObjectDataProvider.MethodParameters>
                <x:TypeExtension TypeName="local:Helpers.Directions"/>
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>
        <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="ConstsWalkingMethod">
            <ObjectDataProvider.MethodParameters>
                <x:TypeExtension TypeName="local:Helpers.WalkingMethod"/>
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>
        <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="ConstsWaypoints">
            <ObjectDataProvider.MethodParameters>
                <x:TypeExtension TypeName="local:Helpers.WaypointType"/>
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>
        <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="ConstsRopes">
            <ObjectDataProvider.MethodParameters>
                <x:TypeExtension TypeName="local:Helpers.ToolsRope"/>
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>
        <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="ConstsShovels">
            <ObjectDataProvider.MethodParameters>
                <x:TypeExtension TypeName="local:Helpers.ToolsShovel"/>
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>
        <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="ConstMonsterAttack">
            <ObjectDataProvider.MethodParameters>
                <x:TypeExtension TypeName="local:Helpers.MonsterAttack"/>
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>
        <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="ConstDesiredStance">
            <ObjectDataProvider.MethodParameters>
                <x:TypeExtension TypeName="local:Helpers.DesiredStance"/>
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>
        <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="ConstDesiredAttack">
            <ObjectDataProvider.MethodParameters>
                <x:TypeExtension TypeName="local:Helpers.DesiredAttack"/>
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>
        <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="ConstAttackMode">
            <ObjectDataProvider.MethodParameters>
                <x:TypeExtension TypeName="local:Helpers.AttackMode"/>
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>

    </Window.Resources>

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="150"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>      
        <Grid.RowDefinitions>
            <RowDefinition Height="30"></RowDefinition>
            <RowDefinition Height="125*" />
            <RowDefinition Height="20"/>
        </Grid.RowDefinitions>      
        <TextBlock Grid.Row="3" VerticalAlignment="Center" HorizontalAlignment="Right" Grid.Column="1">Licensed for panqnik@live.com (31 days left)</TextBlock>
        <Image Grid.Row="0" Stretch="Fill" Height="25" Width="100" Source="/exTibia;component/Graphics/Logo.png"/>
            <Grid Grid.Row="0" Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="60"/>
                    <ColumnDefinition Width="60"/>
                    <ColumnDefinition Width="60"/>
                    <ColumnDefinition Width="60"/>
                    <ColumnDefinition Width="60"/>
                    <ColumnDefinition Width="67"/>
                </Grid.ColumnDefinitions>
                <CheckBox Content="Alerts" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Center" Name="ToggleAlarms"></CheckBox>
                <CheckBox Content="CaveBot" Grid.Column="1" HorizontalAlignment="Center"  VerticalAlignment="Center" Name="ToggleCavebot" Checked="ToggleCavebot_Checked" Unchecked="ToggleCavebot_Unchecked"></CheckBox>
                <CheckBox Name="ToggleHealer" Content="Healer" Grid.Column="2" HorizontalAlignment="Center"  VerticalAlignment="Center" Unchecked="ToggleHealer_Unchecked" Checked="ToggleHealer_Checked"></CheckBox>
                <CheckBox Content="Hotkeys" Grid.Column="3" HorizontalAlignment="Center"  VerticalAlignment="Center"></CheckBox>
                <CheckBox Content="Hud" Grid.Column="4" HorizontalAlignment="Center"  VerticalAlignment="Center"></CheckBox>
                <CheckBox Content="Targeting" Grid.Column="5" HorizontalAlignment="Center"  VerticalAlignment="Center" Name="ToggleTargeting" Checked="ToggleTargeting_Checked" Unchecked="ToggleTargeting_Unchecked"></CheckBox>                     
            </Grid>
        <TreeView Grid.Row="1" Height="300" HorizontalAlignment="Left" Name="treeViewSettings" VerticalAlignment="Top" Width="150" BorderThickness="0" Grid.RowSpan="4" SelectedItemChanged="treeViewSettings_SelectedItemChanged">
            <TreeView.ItemContainerStyle>
                <Style TargetType="{x:Type TreeViewItem}">
                    <Setter Property="IsExpanded" Value="True"/>
                </Style>
            </TreeView.ItemContainerStyle>
            <TreeViewItem Header="Settings" >
                <TreeViewItem Header="Alerts">
                </TreeViewItem >
                <TreeViewItem Header="CaveBot">
                </TreeViewItem >
                <TreeViewItem Header="Healer">
                </TreeViewItem >
                <TreeViewItem Header="Hotkeys">
                </TreeViewItem >
                <TreeViewItem Header="Hud">
                </TreeViewItem >
                <TreeViewItem Header="Input">
                </TreeViewItem >
                <TreeViewItem Header="Looting">
                </TreeViewItem >
                <TreeViewItem Header="Targeting">
                </TreeViewItem >
                <TreeViewItem Header="WarTools">
                </TreeViewItem >              
            </TreeViewItem >
            <TreeViewItem Header="Miscellaneous" >
                <TreeViewItem Header="Character Info">
                </TreeViewItem >
                <TreeViewItem Header="Guild Info">
                </TreeViewItem >
                <TreeViewItem Header="Monster Info">
                </TreeViewItem >
                <TreeViewItem Header="Item Info">
                </TreeViewItem >
                <TreeViewItem Header="Spell Info">
                </TreeViewItem >
                <TreeViewItem Header="Who Is Online?">
                </TreeViewItem >
            </TreeViewItem >
        </TreeView>
        
        <TabControl BorderThickness="0 1 0 0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Grid.Column="1" Grid.Row="1"  Background="White" BorderBrush="Black" Margin="0,0,0,0" Name="tabMenu">
            <TabControl.Resources>
                <Style TargetType="TabItem">
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="TabItem">
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                </Style>
            </TabControl.Resources>

            <!-- Settings -->
            <TabItem Header="Settings" Name="tabSettings">
                <Grid>
                    <TextBlock Name="testBody">Settings</TextBlock>
                    <Button FontSize="10"  Content="Debug Mode" Height="26" HorizontalAlignment="Left" Margin="291,6,0,0" Name="button1" VerticalAlignment="Top" Width="77"/>
                    <TextBox Height="23" HorizontalAlignment="Left" Margin="248,33,0,0" Name="textBox1" VerticalAlignment="Top" Width="120" />
                </Grid>
            </TabItem>

            <!-- Alerts -->
            <TabItem Header="Alerts" Name="tabAlerts">
                <ListBox  ItemsSource="{x:Static local:Modules.Alerts.AlarmList}"  BorderThickness="0 0 0 0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" ScrollViewer.HorizontalScrollBarVisibility="Hidden" Name="SafeListListBox" >
                    <ListBox.ItemContainerStyle>
                        <Style TargetType="ListBoxItem">
                            <Style.Triggers>
                                <Trigger Property="IsSelected" Value="True" >
                                    <Setter Property="Foreground" Value="Black" />
                                </Trigger>
                            </Style.Triggers>
                            <Style.Resources>
                                <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="Transparent"/>
                            </Style.Resources>
                        </Style>
                    </ListBox.ItemContainerStyle>
                    <ListBox.ItemTemplate>                    
                        <DataTemplate>
                            <Grid Height="41">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="40" />
                                </Grid.RowDefinitions>

                                <GroupBox Width="370" FontSize="11"  Header="{Binding Path=Name}">
                                    <Grid VerticalAlignment="Center">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="77"/>
                                            <ColumnDefinition Width="80"/>
                                            <ColumnDefinition Width="70"/>
                                            <ColumnDefinition Width="70"/>
                                            <ColumnDefinition Width="60"/>
                                            <ColumnDefinition Width="67"/>
                                        </Grid.ColumnDefinitions>

                                        <CheckBox Grid.Column="0" Content="Play Sound" FontSize="10" IsChecked="{Binding Path=PlaySound}"></CheckBox>
                                        <CheckBox Grid.Column="1" Content="Focus Client" FontSize="10" IsChecked="{Binding Path=FocusClient}" ></CheckBox>
                                        <CheckBox Grid.Column="2" Content="Pause Bot"  FontSize="10" IsChecked="{Binding Path=PauseBot}"></CheckBox>
                                        <CheckBox Grid.Column="3" Content="Disconnect" FontSize="10" IsChecked="{Binding Path=Disconnect}" ></CheckBox>
                                        <Button Grid.Column="4" Content="SafeList" Visibility="{Binding Path=ButtonVisibility}" FontSize="10" VerticalAlignment="Center" Height="16" Padding="0 0 0 0" Width="50" Name="AlertsSafeList" Click="AlertsSafeList_Click"></Button>
                                    </Grid>
                                </GroupBox>
                                
                                
                            </Grid>
                        </DataTemplate>
                    </ListBox.ItemTemplate>

                </ListBox>
            </TabItem>

            <!-- CaveBot -->
            <TabItem Header="CaveBot" Name="tabCaveBot">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="170" />
                        <ColumnDefinition Width="150*" />
                    </Grid.ColumnDefinitions>                  
                    <Grid Grid.Column="0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="180" />
                            <RowDefinition Height="20" />
                            <RowDefinition Height="50*" />
                        </Grid.RowDefinitions>

                        <GroupBox FontSize="11" Header="Waypoints">
                            <ListBox Grid.Row="0"  ItemsSource="{Binding WaypointList, Source={x:Static local:Modules.CaveBot.Instance}}" Name="CavebotWptList">
                                <ListBox.ItemContainerStyle>                                
                                    <Style TargetType="ListBoxItem"> 
                                        <EventSetter Event="MouseDoubleClick" Handler="WayListDBClick"/>
                                    </Style>
                                </ListBox.ItemContainerStyle>
                                <ListBox.ItemTemplate>                                
                                    <DataTemplate>                                      
                                        <Grid Height="15">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="15" />
                                            </Grid.RowDefinitions>
                                            <Grid.ContextMenu>
                                                <ContextMenu>
                                                    <MenuItem Name="CavebotSetActive" Header="Set as current" Click="CavebotSetActive_Click" />
                                                    <MenuItem Name="CavebotEditBtn" Header="Edit" Click="CavebotEditBtn_Click" />
                                                    <MenuItem Name="CavebotDeleteBtn" Header="Remove" Click="CavebotDeleteBtn_Click" />
                                                    <MenuItem Name="CavebotDeleteAllBtn" Header="Remove All" Click="CavebotDeleteAllBtn_Click" />
                                                </ContextMenu>
                                            </Grid.ContextMenu>
                                            <TextBlock FontSize="10"  Grid.Row="0" Text="{Binding Path=ListBoxItem}" HorizontalAlignment="Left"/>
                                        </Grid>
                                    </DataTemplate>
                                </ListBox.ItemTemplate>
                            </ListBox>
                        
                        
                        
                        </GroupBox>

                        <Grid Grid.Row="1">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="30" />
                                <ColumnDefinition Width="65" />
                                <ColumnDefinition Width="65" />
                            </Grid.ColumnDefinitions>
                            <Button FontSize="9" Grid.Column="0" Content="Add" Height="20" Name="CaveBotAddWpt" Click="CaveBotAddWpt_Click"></Button>
                            <ComboBox FontSize="10" SelectedIndex="0" Grid.Column="1" ItemsSource="{Binding Source={StaticResource ConstsWaypoints}}" Name="CavebotWptType">

                            </ComboBox>
                            <ComboBox  FontSize="10" SelectedIndex="8" Grid.Column="2" ItemsSource="{Binding Source={StaticResource ConstsDirections}}" Name="CavebotWptDirection">
                                
                            </ComboBox>
                        </Grid>



                        <GroupBox FontSize="11"  Grid.Row="2" Header="Tools" Height="66" Margin="0,0,0,0">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="20" />
                                    <RowDefinition Height="20" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="60" />
                                    <ColumnDefinition Width="40*" />
                                </Grid.ColumnDefinitions>
                                <TextBlock FontSize="10"  Grid.Column="0" Grid.Row="0" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="0 0 5 0" >Shovel</TextBlock>
                                <ComboBox FontSize="10" Grid.Row="0" Grid.Column="1" SelectedIndex="0" ItemsSource="{Binding Source={StaticResource ConstsShovels}}"  ></ComboBox>
                                <TextBlock FontSize="10"  Grid.Column="0" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="0 0 5 0" >Rope</TextBlock>
                                <ComboBox FontSize="10" Grid.Row="1" Grid.Column="1"  SelectedIndex="0" ItemsSource="{Binding Source={StaticResource ConstsRopes}}"></ComboBox>
                            </Grid>
                        </GroupBox>

                    </Grid>

                    <Grid Grid.Column="1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="120" />
                            <RowDefinition Height="140*" />
                        </Grid.RowDefinitions>

                        <GroupBox FontSize="11" Header="Pathfinder">                        
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="20" />
                                    <RowDefinition Height="20" />
                                    <RowDefinition Height="20" />
                                    <RowDefinition Height="40*" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="100" />
                                    <ColumnDefinition Width="100" />
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Grid.Row="2" VerticalAlignment="Center" Margin="10 0 0 0" >Walking method</TextBlock>
                                <ComboBox FontSize="10" Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="2" ItemsSource="{Binding Source={StaticResource ConstsWalkingMethod}}"  Name="CavebotWalkingMethodCmb" SelectionChanged="CavebotWalkingMethodCmb_SelectionChanged"></ComboBox>
                                <Button FontSize="9" Grid.Column="0" Width="90" Content="Edit Walk IDs" VerticalAlignment="Center" HorizontalAlignment="Center" Name="CavebotWalkableIdsbtn" Click="CavebotWalkableIdsbtn_Click"></Button>
                                <Button FontSize="9" Grid.Row="1" Grid.Column="0"  Width="90"  Content="Edit NonWalk IDs" VerticalAlignment="Center" HorizontalAlignment="Center" Name="CavebotNonWalkableIdsbtn" Click="CavebotNonWalkableIdsbtn_Click"></Button>
                                <Button FontSize="9" Grid.Row="0" Grid.Column="1"  Width="90"  Content="Draw Map/Way" VerticalAlignment="Center" HorizontalAlignment="Center" Name="CavebotDrawingOptions" Click="CavebotDrawingOptions_Click"></Button>
                                <Button FontSize="9" Grid.Row="1" Grid.Column="1"  Width="90"  Content="Advanced Options" VerticalAlignment="Center" HorizontalAlignment="Center" Name="CavebotPathAdvanced"></Button>
                               


                                <Grid Grid.Row="3" Grid.ColumnSpan="3">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="20" />
                                        <RowDefinition Height="20" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="105" />
                                        <ColumnDefinition Width="95" />
                                    </Grid.ColumnDefinitions>


                                    <CheckBox FontSize="10" Content="Walk by players" Grid.Column="0" Grid.Row="0" Margin="10 0 0 0"  VerticalAlignment="Center" Name="CavebotWalkPlayersCheckbox" Checked="CavebotWalkPlayersCheckbox_Checked" Unchecked="CavebotWalkPlayersCheckbox_Unchecked"></CheckBox>
                                    <CheckBox FontSize="10" Content="Walk by fields" Grid.Column="0" Grid.Row="1" Margin="10 0 0 0"  VerticalAlignment="Center" Name="CavebotWalkFieldsCheckbox" Checked="CavebotWalkFieldsCheckbox_Checked" Unchecked="CavebotWalkFieldsCheckbox_Unchecked"></CheckBox>

                                    <TextBlock FontSize="10" Grid.Column="1" VerticalAlignment="Center" Margin="0 0 20 0" HorizontalAlignment="Right"  Grid.Row="0">Skip nodes:</TextBlock>
                                    <TextBox FontSize="10" Grid.Column="1" HorizontalAlignment="Right" Grid.Row="0" Height="20" Width="15" Name="CavebotSkipNodestxt" TextChanged="CavebotSkipNodestxt_TextChanged"></TextBox>
                                    <TextBlock FontSize="10" Grid.Column="1" VerticalAlignment="Center" Margin="5,0,0,0"   HorizontalAlignment="Left"  Grid.Row="1">Max attempts:</TextBlock>
                                    <TextBox FontSize="10" Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Right" Grid.Row="1" Width="15" Name="CavebotMaxAttemptstxt" TextChanged="CavebotMaxAttemptstxt_TextChanged"></TextBox>
                                    <Grid Grid.Column="0" Grid.Row="0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="40" />
                                            <ColumnDefinition Width="65" />
                                        </Grid.ColumnDefinitions>
                                        
                                    </Grid>
                                        
                                        <Grid Grid.Column="0" Grid.Row="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="40" />
                                            <ColumnDefinition Width="65" />
                                        </Grid.ColumnDefinitions>
                                        
                                    </Grid>
                                </Grid>                   
                            </Grid>                         
                        </GroupBox>

                        <GroupBox FontSize="11"  Header="Special Areas" Grid.Row="1">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="105*" />
                                </Grid.ColumnDefinitions>
                                <Grid.ContextMenu>
                                    <ContextMenu>
                                        <MenuItem Name="SpecialAreaAdd" Header="Add new special area" Click="SpecialAreaAdd_Click" />
                                    </ContextMenu>
                                </Grid.ContextMenu>

                                <ListBox Grid.ColumnSpan="2" Margin="0,0,0,0"   ItemsSource="{Binding SpecialAreasList, Source={x:Static local:Modules.CaveBot.Instance}}" Name="CavebotAreasList">
                                    <ListBox.ItemTemplate>
                                        <DataTemplate>
                                            <Grid Height="25">
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="25" />
                                                </Grid.RowDefinitions>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="60" />
                                                    <ColumnDefinition Width="100*" />
                                                </Grid.ColumnDefinitions>
                                                <Grid.ContextMenu>
                                                    <ContextMenu>
                                                        <MenuItem Name="SpecialAreaEdit" Header="Edit" Click="SpecialAreaEdit_Click" />
                                                        <MenuItem Name="SpecialAreaRemove" Header="Remove" Click="SpecialAreaRemove_Click" />
                                                    </ContextMenu>
                                                </Grid.ContextMenu>
                                                <TextBlock FontSize="10"  Grid.Column="0" VerticalAlignment="Center" HorizontalAlignment="Center" FontFamily="Consolas" Text="{Binding Path=SpecialName}"/>
                                                <TextBlock FontSize="10" Grid.Column="1" VerticalAlignment="Center" HorizontalAlignment="Center" Text="{Binding Path=SpecialCords}"/>
                                            
                                            </Grid>
                                        </DataTemplate>
                                    </ListBox.ItemTemplate>



                                </ListBox>
                            </Grid>
                        </GroupBox>
                    </Grid>
                </Grid>
            </TabItem>

            <!-- Healer -->
            <TabItem Header="Healer" Name="tabHealer" >
                <Grid >
                    <Grid.RowDefinitions>
                        <RowDefinition Height="20" />
                        <RowDefinition Height="230*" />
                    </Grid.RowDefinitions>     
                    
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="160" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        
                            <Grid Grid.Column="0" HorizontalAlignment="Left">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="80" />
                                <ColumnDefinition Width="80" />
                            </Grid.ColumnDefinitions>
                            <Button FontSize="10"  Name="HealerNewRule" Grid.Column="0" Content="Add heal rule" Height="18" Width="80" VerticalAlignment="Center" HorizontalAlignment="Left" Click="HealerNewRule_Click"></Button>
                            <Button FontSize="10" Name="HealerClean" Grid.Column="1" Content="Clean rules" Height="18" Width="80" VerticalAlignment="Center" HorizontalAlignment="Left" Click="HealerClean_Click"></Button>
                        </Grid>
                    </Grid>
                    <ListBox BorderThickness="0 0 0 0" ItemsSource="{Binding HealRules, Source={x:Static local:Modules.Healer.Instance} }" HorizontalAlignment="Stretch" Margin="0,20,0,0" Name="HealingListBox" VerticalAlignment="Stretch" Grid.Row="1">
                        <ListBox.ItemContainerStyle>
                            <Style TargetType="ListBoxItem">
                                <Style.Triggers>
                                    <Trigger Property="IsSelected" Value="True" >
                                        <Setter Property="Foreground" Value="Black" />
                                    </Trigger>
                                </Style.Triggers>
                                <Style.Resources>
                                    <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="Transparent"/>
                                </Style.Resources>
                            </Style>
                        </ListBox.ItemContainerStyle>
                        <ListBox.ItemTemplate>                         
                            <DataTemplate>
                                <Grid Height="45">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="15" />
                                        <RowDefinition Height="15*" />
                                        <RowDefinition Height="15*" />
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="90"/>
                                        <ColumnDefinition Width="60"/>
                                        <ColumnDefinition Width="130"/>
                                        <ColumnDefinition Width="90*"/>
                                    </Grid.ColumnDefinitions>

                                    <TextBlock FontSize="10" Grid.Column="0" Grid.Row="1" Text="{Binding Path=TextName}" HorizontalAlignment="Center" FontWeight="Bold"/>
                                    <TextBlock FontSize="10" HorizontalAlignment="Left" Grid.Column="1" Grid.Row="0" Text="HP Range:" FontWeight="Bold"/>
                                    <TextBlock FontSize="10" HorizontalAlignment="Left" Grid.Column="1" Grid.Row="1" Text="MP Range:" FontWeight="Bold"/>
                                    <TextBlock FontSize="10" HorizontalAlignment="Left" Grid.Column="1" Grid.Row="2" Text="Priority:" FontWeight="Bold"/>
                                    <TextBlock FontSize="10" HorizontalAlignment="Left" Grid.Column="2" Grid.Row="0" Text="{Binding Path=TextHPrange}" />
                                    <TextBlock FontSize="10" HorizontalAlignment="Left" Grid.Column="2" Grid.Row="1" Text="{Binding Path=TextMPrange}" />
                                    <TextBlock FontSize="10" HorizontalAlignment="Left" Grid.Column="2" Grid.Row="2" Text="{Binding Path=TextPriority}" />

                                    <Grid Grid.Column="4" Grid.Row="1" Height="48" HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="45"/>
                                            <ColumnDefinition Width="45"/>
                                        </Grid.ColumnDefinitions>
                                        <Button Name="HealingEditBtn" Grid.Column="0" Height="16" Width="40" Content="Edit" FontSize="9" Click="HealingEditBtn_Click"></Button>
                                        <Button Name="HealingDeleteBtn" Grid.Column="1" Height="16" Width="40" Content="Remove" FontSize="9" Click="HealingDeleteBtn_Click"></Button>
                                    </Grid>
                                </Grid>
                            </DataTemplate>
                        </ListBox.ItemTemplate>

                    </ListBox>
                </Grid>
            </TabItem>

            <!-- Hotkeys -->
            <TabItem Header="Hotkeys" Name="tabHotkeys">
                <Grid>
                    <TextBlock>Hotkeys</TextBlock>
                </Grid>
            </TabItem>

            <!-- Hud -->
            <TabItem Header="Hud" Name="tabHud">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="20" />
                        <RowDefinition Height="230*" />
                    </Grid.RowDefinitions>

                    <Grid Grid.Row="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="160" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>

                        <Grid Grid.Row="0" HorizontalAlignment="Left">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="80" />
                                <ColumnDefinition Width="80" />
                            </Grid.ColumnDefinitions>
                            <Button FontSize="10"  Name="HudNewRule" Grid.Column="0" Content="Add new HUD" Height="18" Width="80" VerticalAlignment="Center" HorizontalAlignment="Left"></Button>
                            <Button FontSize="10" Name="HudClean" Grid.Column="1" Content="Clean HUDs" Height="18" Width="80" VerticalAlignment="Center" HorizontalAlignment="Left"></Button>
                        </Grid>
                    </Grid>

                    <Grid Grid.Row="1">
                        <ListBox BorderThickness="0 0 0 0" ItemsSource="{Binding HudScripts, Source={x:Static local:Modules.Hud.Instance} }" HorizontalAlignment="Stretch" Margin="0,0,0,0" Name="HudsListBox" VerticalAlignment="Stretch">
                            <ListBox.ItemContainerStyle>
                                <Style TargetType="ListBoxItem">
                                    <Style.Triggers>
                                        <Trigger Property="IsSelected" Value="True" >
                                            <Setter Property="Foreground" Value="Black" />
                                        </Trigger>
                                    </Style.Triggers>
                                    <Style.Resources>
                                        <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="Transparent"/>
                                    </Style.Resources>
                                </Style>
                            </ListBox.ItemContainerStyle>
                            <ListBox.ItemTemplate>
                                <DataTemplate>
                                    <Grid Height="35">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="25" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="90"/>
                                            <ColumnDefinition Width="60"/>
                                            <ColumnDefinition Width="130"/>
                                            <ColumnDefinition Width="90*"/>
                                        </Grid.ColumnDefinitions>

                                        

                                        <Grid Grid.Column="4" Grid.Row="1" Height="48" HorizontalAlignment="Center" VerticalAlignment="Center">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="45"/>
                                                <ColumnDefinition Width="45"/>
                                            </Grid.ColumnDefinitions>
                                            <Button Name="HudgEditBtn" Grid.Column="0" Height="16" Width="40" Content="Edit" FontSize="9"></Button>
                                            <Button Name="HudDeleteBtn" Grid.Column="1" Height="16" Width="40" Content="Remove" FontSize="9"></Button>
                                        </Grid>
                                    </Grid>
                                </DataTemplate>
                            </ListBox.ItemTemplate>
                        </ListBox>



                    </Grid>


                </Grid>
            </TabItem>


            <!-- Input -->
            <TabItem Header="Input" Name="tabInput">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="170" />
                        <RowDefinition Height="50" />
                    </Grid.RowDefinitions>

                    <Grid Row="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="185"/>
                            <ColumnDefinition Width="185"/>
                        </Grid.ColumnDefinitions>

                        <GroupBox Header="Mouse settings" FontSize="9" Grid.Column="0">
                            <Grid  Margin="0 5 0 0" >
